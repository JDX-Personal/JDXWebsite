<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Stupid" table="article">
        <id column="ArticleId" name="id" type="java.lang.Integer" />
        <property column="PageName" name="pageName" type="java.lang.String" />
        <property column="Title" name="title" type="java.lang.String" />
        <property column="Overview" name="overview" type="java.lang.String" />
        <property column="CreatedDate" name="createdDate" type="java.lang.String" />
        <property column="LasteUpdated" name="lastUpdated" type="java.lang.String" />
    </class>
</hibernate-mapping>